<form id="general_settings" xmlns:tal="http://axiomstack.com/tale">
  <h2>General Settings</h2>
  <fieldset class="axContacts">
    <table>
        <tr>
            <td class="settingLabel"><label for="contacts">Axiom Contacts (Displayed on login screen)</label></td>
            <td class="contactsRow"><textarea id="contacts" name="contacts" tal:content="this.contacts||''"> </textarea></td>	
        </tr>
    </table>
  </fieldset>
</form>    
